void main() {
	uint64 addr = memory_allocate_units(4, 32);
	memory_buffer<float> buf(addr, 32);
	for (uint i = 0; i < 32; i++) buf[i] = i * 2;
	string output;
	for (uint i = 0; i < 32; i++) output += buf[i] + ". ";
	memory_free(addr);
	alert("test", output);
}
